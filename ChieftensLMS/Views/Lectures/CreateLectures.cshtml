
@{
    ViewBag.Title = "CreateLectures";
}

<h2>CreateLectures</h2>

<a href="~/Lectures/Index/@Model">Lektioner för kurs</a>
<div ng-controller="assignmentCreateController" ng-init="GetCourseAssigments(@Model)">

	<div class="form-group">
		<label class="control-label col-md-2" for="Name">Namn</label>
		<div class="col-md-10">
			<input id="Name" type="text" class="form-control" ng-model="Name" />
		</div>
	</div>

	<div class="form-group">
		<label class="control-label col-md-2" for="Description">Beskrivning</label>
		<div class="col-md-10">
			<input id="Description" type="text" class="form-control" ng-model="Description" />
		</div>
	</div>

	<div class="form-group">
		<label class="control-label col-md-2" for="Description">Tid i minuter</label>
		<div class="col-md-10">
			<input id="TimeInMin" type="text" class="form-control" ng-model="TimeInMin" />
		</div>
	</div>
</div>