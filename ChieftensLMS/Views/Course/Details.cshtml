@model int?


<div class="well">

	@if (User.IsInRole("Teacher"))
			{
				Html.RenderPartial("_Single_Teacher", Model);
			}
			else
			{
				Html.RenderPartial("_Single", Model);
			}

	<a href="/Course/Overview" class="btn btn-default">Tillbaka</a>
	<a href="/SharedFile/Course/@Model" class="btn btn-default">Delade filer</a>
	<a href="/Course/Users/@Model" class="btn btn-default">Medlemmar</a>
	<a href="/Lectures/Index/@Model" class="btn btn-default">Lektioner</a>
	<a href="/Assignment/Index/@Model" class="btn btn-default">Inlämningsuppgifter</a>
</div>




